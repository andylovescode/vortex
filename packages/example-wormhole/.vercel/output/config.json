{
  "version": 3,
  "routes": [
    {
      "src": "/api/add",
      "dest": "/functions/api-api/add.func",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/",
      "dest": "/functions/route-index.func"
    },
    {
      "src": "/docs/*",
      "dest": "/functions/route-docs/[...page].func"
    },
    {
      "src": "/*",
      "dest": "/functions/route-[...404].func"
    },
    {
      "src": "/(.*)",
      "dest": "/functions/index.func"
    }
  ]
}