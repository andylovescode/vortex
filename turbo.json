{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"outputs": ["dist/**"],
			"dependsOn": ["^build"]
		},
		"fmt": {
			"persistent": true,
			"cache": false,
			"dependsOn": ["^fmt"]
		},
		"dev": {
			"persistent": true,
			"cache": false,
			"dependsOn": ["^build"]
		}
	}
}
