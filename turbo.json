{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"@vortexjs/discovery#build": {
			"outputs": [
				"index.js",
				"index.d.ts",
				"*.node"
			],
			"passThroughEnv": [
				"RUSTUP_HOME",
				"RUSTUP_TOOLCHAIN"
			],
			"outputLogs": "new-only"
		},
		"build": {
			"outputs": [
				"dist/**"
			],
			"dependsOn": [
				"^build"
			],
			"outputLogs": "new-only"
		},
		"@vortexjs/discovery#fmt": {
			"persistent": false,
			"cache": true,
			"passThroughEnv": [
				"RUSTUP_HOME",
				"RUSTUP_TOOLCHAIN"
			],
			"outputLogs": "new-only",
			"inputs": [
				"$TURBO_DEFAULT$",
				"!index.js",
				"!index.d.ts",
				"!target/release/**"
			]
		},
		"fmt": {
			"persistent": false,
			"cache": true,
			"outputLogs": "new-only"
		},
		"dev": {
			"persistent": true,
			"cache": false,
			"dependsOn": [
				"^build"
			]
		},
		"test": {
			"persistent": false,
			"cache": true,
			"dependsOn": [
				"^build"
			]
		}
	}
}
